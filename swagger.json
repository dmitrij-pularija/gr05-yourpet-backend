					"password": {
						"type": "integer",
						"minLength": 6,
						"maxLength": 16,
						"pattern": " ^ ^ (?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.{6,16}) $",
						"example": "qWerty123"
